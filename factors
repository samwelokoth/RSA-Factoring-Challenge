def factorize(number):
    factors = []
    for i in range(2, number):
        if number % i == 0:
            factors.append(i)
            factors.append(number // i)
            break
    return factors


def main():
    with open("file.txt", "r") as file:
        numbers = file.readlines()
        for number in numbers:
            n = int(number.strip())
            factors = factorize(n)
            print(f"{n}={factors[0]}*{factors[1]}")


if __name__ == "__main__":
    main()
